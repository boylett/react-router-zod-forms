{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "outDir": "./dist",
    "rootDir": "./src",
    "moduleResolution": "node10",
    "jsx": "react",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noEmit": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "declaration": true
  }
}